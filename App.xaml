<Application
    x:Class="Starter.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:conv="clr-namespace:Starter.Converters"
    xmlns:local="clr-namespace:Starter"
    StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:StringToBrushConverter x:Key="StringToBrushConverter" />
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
            <local:BooleanToBlurEffectConverter x:Key="BooleanToBlurEffectConverter" />
            <local:PathToIconConverter x:Key="PathToIconConverter" />
            <conv:BoolToVisibilityConverter x:Key="BoolToVisibility" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
